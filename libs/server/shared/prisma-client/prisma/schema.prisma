datasource db {
  provider = "sqlite"
  url      = "file:./db/database.db"
}

generator client {
  provider = "prisma-client-js"
  output   = "../../../../../../../node_modules/.prisma/client"
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  email     String   @unique 
  name      String?
}