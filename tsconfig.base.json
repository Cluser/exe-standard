{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "useUnknownInCatchVariables": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noPropertyAccessFromIndexSignature": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@exe/client/client-web/core/auth-keycloak": ["libs/client/client-web/core/auth-keycloak/src/index.ts"],
      "@exe/client/client-web/core/core": ["libs/client/client-web/core/core/src/index.ts"],
      "@exe/client/client-web/core/http-errors-handler": [
        "libs/client/client-web/core/http-errors-handler/src/index.ts"
      ],
      "@exe/client/client-web/core/store": ["libs/client/client-web/core/store/src/index.ts"],
      "@exe/client/client-web/core/transloco": ["libs/client/client-web/core/transloco/src/index.ts"],
      "@exe/client/client-web/feature/feature-dashboard": [
        "libs/client/client-web/feature/feature-dashboard/src/index.ts"
      ],
      "@exe/client/client-web/shared/guards": ["libs/client/client-web/shared/guards/src/index.ts"],
      "@exe/client/client-web/shared/loader": ["libs/client/client-web/shared/loader/src/index.ts"],
      "@exe/client/client-web/shared/local-storage": ["libs/client/client-web/shared/local-storage/src/index.ts"],
      "@exe/client/client-web/shared/modal": ["libs/client/client-web/shared/modal/src/index.ts"],
      "@exe/client/client-web/shared/navigation": ["libs/client/client-web/shared/navigation/src/index.ts"],
      "@exe/client/client-web/shared/not-authorized": ["libs/client/client-web/shared/not-authorized/src/index.ts"],
      "@exe/client/client-web/shared/not-found": ["libs/client/client-web/shared/not-found/src/index.ts"],
      "@exe/client/client-web/shared/resolvers": ["libs/client/client-web/shared/resolvers/src/index.ts"],
      "@exe/client/client-web/shared/session-storage": ["libs/client/client-web/shared/session-storage/src/index.ts"],
      "@exe/client/client-web/shared/shared": ["libs/client/client-web/shared/shared/src/index.ts"],
      "@exe/client/shared/components": ["libs/client/shared/components/src/index.ts"],
      "@exe/client/shared/data-access": ["libs/client/shared/data-access/src/index.ts"],
      "@exe/client/shared/socket-io": ["libs/client/shared/socket-io/src/index.ts"],
      "@exe/server/server-api/core/auth-keycloak": ["libs/server/server-api/core/auth-keycloak/src/index.ts"],
      "@exe/server/server-api/core/core": ["libs/server/server-api/core/core/src/index.ts"],
      "@exe/server/server-api/core/logger-pino": ["libs/server/server-api/core/logger-pino/src/index.ts"],
      "@exe/server/server-api/feature/feature-user": ["libs/server/server-api/feature/feature-user/src/index.ts"],
      "@exe/server/server-api/shared/socket-io": ["libs/server/server-api/shared/socket-io/src/index.ts"],
      "@exe/server/shared/pagination": ["libs/server/shared/pagination/src/index.ts"],
      "@exe/server/shared/prisma-client": ["libs/server/shared/prisma-client/src/index.ts"]
    }
  },
  "include": ["apps", "libs"],
  "exclude": ["node_modules"]
}
